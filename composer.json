{
    "name": "econtract/toolbox",
    "description": "E-Contract BVBA toolbox API package",
    "version": "3.0.0",
    "keywords": ["API", "E-Contract", "Aanbieders", "Toolbox"],
    "homepage": "https://aanbieders.be",
    "license": "proprietary",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        }
    ],

    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": ">=6.0",
        "ixudra/curl": "~6.0",
        "vlucas/phpdotenv": "^5.2"
    },

    "autoload": {
        "psr-4": {
            "Econtract\\Toolbox\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Econtract\\Toolbox\\ToolboxServiceProvider"
            ],
            "aliases": {
                "Toolbox": "Econtract\\Toolbox\\Facades\\Toolbox"
            }
        }
    },

    "minimum-stability": "stable"
}
